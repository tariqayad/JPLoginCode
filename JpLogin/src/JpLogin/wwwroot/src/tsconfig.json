        "module": "commonjs",
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5"
    },
    "include": [
        "src/**/*",
        "src/*"
    ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
